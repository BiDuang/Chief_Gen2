<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Chief_Reloaded"
             x:Class="Chief_Reloaded.App">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>
    <Application.Styles>
        <Style Selector="TextBlock">
            <Setter Property="FontFamily" Value="Microsoft YaHei, Menlo, Noto Sans CJK SC" />
        </Style>
        <Style Selector="TextBox">
            <Setter Property="FontFamily" Value="Microsoft YaHei, Menlo, Noto Sans CJK SC" />
        </Style>
        <FluentTheme Mode="Dark" />
        <StyleInclude Source="avares://Chief_Reloaded/Icons.axaml" />
    </Application.Styles>
</Application>